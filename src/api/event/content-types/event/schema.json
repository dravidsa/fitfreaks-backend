{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "events",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "tickets": {
      "type": "biginteger",
      "required": true
    },
    "location": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "events"
    },
    "eventType": {
      "type": "enumeration",
      "enum": [
        "none",
        "popular",
        "featured"
      ],
      "default": "none"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "sports",
        "charity",
        "private",
        "corporate",
        "festival",
        "party",
        "concert"
      ],
      "required": true
    },
    "organizer": {
      "type": "string",
      "required": true
    },
    "sportType": {
      "type": "enumeration",
      "enum": [
        "Running",
        "Walking",
        "Cycling",
        "Swimming",
        "Triathlon",
        "Hiking"
      ]
    },
    "attendee_catagories": {
      "type": "component",
      "repeatable": true,
      "component": "site-component.attendee-catagories"
    },
    "event_catagories": {
      "type": "component",
      "repeatable": true,
      "component": "site-component.event-categories"
    },
    "charges": {
      "type": "component",
      "repeatable": false,
      "component": "site-component.charges"
    },
    "terms": {
      "type": "text"
    },
    "location_GPS": {
      "type": "string"
    },
    "location_address": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive"
      ]
    },
    "accommodation": {
      "type": "enumeration",
      "enum": [
        "Single Room",
        "Shared Room"
      ]
    },
    "GST_flag": {
      "type": "boolean"
    },
    "GST_percent": {
      "type": "decimal"
    },
    "GST_source": {
      "type": "enumeration",
      "enum": [
        "End_User",
        "Event_organizer"
      ]
    },
    "platform_fees_flag": {
      "type": "boolean"
    },
    "platform_fees_percent": {
      "type": "decimal"
    },
    "platform_fees_source": {
      "type": "enumeration",
      "enum": [
        "End_User",
        "Event_organizer"
      ]
    },
    "event_message": {
      "type": "string"
    },
    "tshirt_flag": {
      "type": "boolean"
    },
    "tshirt_sizes": {
      "type": "string"
    },
    "accomodation_options": {
      "type": "string"
    },
    "accomodation_flag": {
      "type": "boolean"
    },
    "dob_flag": {
      "type": "boolean"
    },
    "profession_flag": {
      "type": "boolean"
    },
    "document_flag": {
      "type": "boolean"
    }
  }
}
